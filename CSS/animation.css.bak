.animate{
	height:300px;
	width:300px;
	position:absolute;
	background-color:red;
	animation-name:color_change;
	animation-duration:5s;
	animation-iteration-count:4; /*by default, transition occurs once*/
	animation-direction:alternate;
	animation-timing-function:linear;
	animation-delay:3s;
}


@keyframes color_change{
	from{
		background-color:red;
		border-radius:100%;
		left:0;
		top:0;
	}
	
	 50%{
		 background-color:green;
		 border-radius:50%;
		 left:80%;
		 top:0;
	 }
	 
	 to{
		 background-color:blue;
		 border-radius:0%;
		 left:80%;
		 top:80%;
	 }
}

@keyframes rotate{
	from{
		transform:rotate(0deg);
	}
	
	to{
		transform:rotate(360deg);
	}
}